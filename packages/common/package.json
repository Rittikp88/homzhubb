{
  "name": "@homzhub/common",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "compile": "tsc -b --incremental",
    "clean": "shx rm -f *.tsbuildinfo && shx rm -rf dist/*",
    "build": "yarn clean && yarn compile",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}"
  }
}