{
  "name": "@homzhub/ffm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "ENVFILE=.env.development react-native start",
    "android:bundle": "ENVFILE=.env.development react-native bundle --platform android --dev false --entry-file packages/ffm/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android": "ENVFILE=.env.development react-native run-android",
    "pod:install": "cd ios && pod install",
    "ios": "ENVFILE=.env.development react-native run-ios",
    "ios:clean": "rm -rf ios/build",
    "ios:bundle": "ENVFILE=.env.development react-native bundle --platform ios --dev false --entry-file packages/ffm/index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:build": "ENVFILE=.env.development yarn ios:clean && yarn ios:bundle",
    "test": "ENVFILE=.env.development jest --coverage --env=jsdom",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,json}'",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@homzhub/common": "0.0.1",
    "@react-native-community/async-storage": "^1.10.0",
    "@react-native-community/art": "^1.2.0",
    "@react-navigation/bottom-tabs": "^5.4.2",
    "@react-navigation/material-top-tabs": "^5.2.16",
    "@react-navigation/native": "^5.3.0",
    "@react-navigation/stack": "^5.3.3",
    "react-dom": "16",
    "react-native": "0.63.3",
    "react-native-bootsplash": "^3.1.5",
    "react-native-config": "^1.3.3",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-safe-area-context": "^1.0.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-restart": "^0.0.22",
    "react-native-vector-icons": "6.6.0"
  },
  "devDependencies": {
    "@types/react-native": "^0.62.7",
    "@types/react-native-vector-icons": "^6.4.5",
    "metro-react-native-babel-preset": "0.59.0"
  }
}
