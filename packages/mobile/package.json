{
  "name": "@homzhub/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.development react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:bundle": "ENVFILE=.env.development react-native bundle --platform android --dev false --entry-file packages/mobile/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build": "ENVFILE=.env.development yarn android:clean && yarn android:bundle && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "android:bundle:prod": "ENVFILE=.env react-native bundle --platform android --dev false --entry-file packages/mobile/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build:prod": "ENVFILE=.env yarn android:clean && yarn android:bundle:prod && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "ios": "ENVFILE=.env.development react-native run-ios",
    "ios:clean": "rm -rf ios/build",
    "pod:install": "cd ios && pod install",
    "ios:build": "ENVFILE=.env.development yarn ios:clean && yarn ios:bundle",
    "ios:bundle": "ENVFILE=.env.development react-native bundle --platform ios --dev false --entry-file packages/mobile/index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:bundle:prod": "ENVFILE=.env react-native bundle --platform ios --dev false --entry-file packages/mobile/index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:build:prod": "ENVFILE=.env yarn ios:clean && yarn ios:bundle:prod",
    "start": "ENVFILE=.env.development react-native start",
    "test": "ENVFILE=.env.development jest --coverage --env=jsdom",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,json}'",
    "sonar": "ts-node sonar-project.ts",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@homzhub/common": "0.0.1",
    "@invertase/react-native-apple-authentication": "^2.1.1",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/art": "^1.2.0",
