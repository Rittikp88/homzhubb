{
  "name": "@homzhub/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file packages/mobile/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build": "yarn android:clean && yarn android:bundle && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "android:clean": "cd android && ./gradlew clean",
    "ios": "react-native run-ios",
    "ios:build": "yarn ios:clean && yarn ios:bundle",
    "ios:bundle": "react-native bundle --platform ios --dev false --entry-file packages/mobile/index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:clean": "rm -rf ios/build",
    "pod:install": "cd ios && pod install",
    "start": "react-native start",
    "test": "jest --coverage --env=jsdom",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,json}'",
    "sonar": "ts-node sonar-project.ts",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@homzhub/common": "0.0.1",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/async-storage": "^1.10.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.5.1",
    "@react-native-community/slider": "^3.0.0",
    "@react-native-community/viewpager": "^4.1.6",
    "@react-navigation/bottom-tabs": "^5.4.2",
    "@react-navigation/native": "^5.3.0",
    "@react-navigation/stack": "^5.3.3",
    "react-dom": "16",
    "react-native": "0.62.2",
    "react-native-collapsible": "^1.5.2",
    "react-native-crypto-js": "^1.0.0",
    "react-native-document-picker": "^3.5.1",
    "react-native-easy-markdown": "^2.0.0",
    "react-native-fbsdk": "^2.0.0",
    "react-native-flash-message": "^0.1.15",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-crop-picker": "^0.32.0",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.27.1",
    "react-native-media-controls": "^2.0.3",
    "react-native-orientation-locker": "^1.1.8",
    "react-native-otp-verify": "^1.0.3",
    "react-native-parallax-scroll-view": "^0.21.3",
    "react-native-permissions": "^2.1.5",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-razorpay": "^2.1.33",
    "react-native-restart": "^0.0.15",
    "react-native-safe-area-context": "^1.0.0",
    "react-native-screens": "^2.7.0",
    "react-native-scrollable-tab-view": "^1.0.0",
    "react-native-slider": "^0.11.0",
    "react-native-smooth-picker": "^1.0.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-step-indicator": "^0.0.11",
    "react-native-svg": "^12.1.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "6.6.0",
    "react-native-video": "^4.4.5",
    "react-native-webview": "^10.8.2",
    "react-native-youtube": "^2.0.1"
  },
  "devDependencies": {
    "@types/react-native": "^0.62.7",
    "@types/react-native-collapsible": "^0.11.0",
    "@types/react-native-fbsdk": "^1.1.0",
    "@types/react-native-linear-gradient": "^2.4.0",
    "@types/react-native-maps": "^0.24.0",
    "@types/react-native-permissions": "^2.0.0",
    "@types/react-native-restart": "^0.0.0",
    "@types/react-native-scrollable-tab-view": "^0.10.1",
    "@types/react-native-snap-carousel": "^3.8.1",
    "@types/react-native-svg-charts": "^5.0.3",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-native-video": "^5.0.1",
    "metro-react-native-babel-preset": "^0.59.0"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
